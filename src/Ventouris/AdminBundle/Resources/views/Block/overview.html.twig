        <thead>
            <tr>
                <th colspan="3">Overview</th>
            </tr>
        </thead>

        <tbody>
            <tr>
                <td class="sonata-ba-list-label">Total Users</td>
                <td>{{ users|length }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}